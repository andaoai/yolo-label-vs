{
  "name": "yolo-labeling-vs",
  "displayName": "YOLO Labeling",
  "description": "A VS Code extension for YOLO dataset labeling",
  "version": "0.0.69",
  "publisher": "andaoai",
  "license": "MIT",
  "icon": "docs/images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/andaoai/yolo-label-vs"
  },
  "keywords": [
    "yolo",
    "labeling",
    "dataset",
    "computer vision",
    "machine learning",
    "annotation",
    "object detection",
    "image annotation",
    "bounding box",
    "deep learning",
    "ai",
    "yaml",
    "keybindings",
    "darknet",
    "neural network",
    "yolov5",
    "yolov8",
    "ultralytics",
    "attention mechanism",
    "transformer",
    "computer vision",
    "pytorch",
    "tensorflow",
    "cv",
    "data labeling",
    "detection",
    "segmentation",
    "coco",
    "instance segmentation",
    "real-time"
  ],
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other",
    "Data Science",
    "Machine Learning",
    "Visualization"
  ],
  "activationEvents": [
    "onStartupFinished",
    "workspaceContains:**/*.yaml",
    "workspaceContains:**/*.yml"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "yolo-labeling-vs.openLabelingPanel",
        "title": "Open YOLO Labeling Panel"
      },
      {
        "command": "yolo-labeling-vs.openLabelingPanelKeyboard",
        "title": "Open YOLO Labeling Panel (Keyboard Shortcut)"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname == .yaml || resourceExtname == .yml",
          "command": "yolo-labeling-vs.openLabelingPanel",
          "group": "navigation@1"
        }
      ],
      "editor/context": [
        {
          "when": "resourceExtname == .yaml || resourceExtname == .yml",
          "command": "yolo-labeling-vs.openLabelingPanel",
          "group": "navigation@1"
        }
      ],
      "editor/title/context": [
        {
          "when": "resourceExtname == .yaml || resourceExtname == .yml",
          "command": "yolo-labeling-vs.openLabelingPanel",
          "group": "navigation@1"
        }
      ]
    },
    "keybindings": [
      {
        "command": "yolo-labeling-vs.openLabelingPanel",
        "key": "ctrl+y",
        "when": "editorTextFocus && resourceExtname == .yaml || resourceExtname == .yml"
      },
      {
        "command": "yolo-labeling-vs.openLabelingPanelKeyboard",
        "key": "ctrl+y",
        "when": "!editorTextFocus || !(resourceExtname == .yaml || resourceExtname == .yml)"
      }
    ]
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js && npm run copy-templates",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack.config.js && npm run copy-templates",
    "watch": "cross-env NODE_ENV=development webpack --watch --config webpack.config.js && npm run copy-templates",
    "copy-templates": "node scripts/copy-templates.js",
    "vscode:prepublish": "npm run build"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.17.32",
    "@types/vscode": "^1.85.0",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "chokidar": "^3.5.3",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.23.0",
    "svg2png-cli": "^1.1.1",
    "svgexport": "^0.4.2",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "jimp": "^0.22.10",
    "js-yaml": "^4.1.0"
  }
}
